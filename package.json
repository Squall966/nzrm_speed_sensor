{
  "name": "nzrm_speed_sensor_2023",
  "version": "1.0.19",
  "description": "The Speed Detector/Sensor for New Zealand Rugby Museum 2023.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder && npm version patch"
  },
  "repository": "https://github.com/Squall966/nzrm_speed_sensor",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": {
    "name": "Squall Lion",
    "email": "lion@inccreative.co.nz",
    "url": "https://inccreative.co.nz/"
  },
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "appId": "com.inc.nzrm_speed_sensor_2023",
    "productName": "NZRM Speed Sensor 2023",
    "asar": false,
    "directories": {
      "output": "releases/${platform}/${arch}"
    },
    "linux": {
      "category": "Utility",
      "target": "AppImage",
      "icon": "docs/256x256-icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "docs/icon.ico"
    },
    "nsis": {
      "perMachine": true
    }
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "^7.3.0",
    "electron": "^23.1.2",
    "electron-builder": "^23.3.3",
    "electron-reloader": "^1.2.3",
    "sass": "^1.54.5"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-shutdown-command": "^2.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.2.1",
    "gsap": "^3.11.3",
    "jquery": "^3.6.1",
    "mongoose": "^6.5.4",
    "phaser": "^3.55.2",
    "progressbar.js": "^1.1.0",
    "reset-css": "^5.0.1",
    "socket.io-client": "^4.5.2",
    "uuid": "^8.3.2"
  }
}
